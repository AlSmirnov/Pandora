<pandora-model>

  <World>

    <Person order="1">
      <first_name pos="1" fsize="20" />
      <last_name pos="+1" fsize="25" />
      <birth_day pos="|+1" />
      <birth_place pos="+0.10up" />
      <father_name pos="|up+0.3" />
      <mother_name pos="up+0.1" />
      <sex pos="|left" />
      <exit_day pos="left,hide" />
    </Person>

    <Community order="2">
      <name pos="1" />
      <founder pos="|+1" />
      <origin_place pos="+1" />
      <origin_date pos="+1" />
    </Community>

  </World>

  <Pandora name="Pandora Node">

    <Hashed>
      <panhash pos="|255" view="phash" />
    </Hashed>

    <HashedCreated>
      <creator pos="255.1" view="hex" />
    </HashedCreated>

    <Parameter order="101">
      <name pos="1" fsize="32" />
      <type pos="+1" />
      <section pos="+1" />
      <desc pos="|+1" fsize="64" />
      <value pos="|+1" fsize="64" />
      <setting pos="|+1" fsize="64" />
    </Parameter>

    <Key>
      <kind view="byte" />
      <owner view="panhash" />
      <date view="time" />
      <expire view="time" />
      <body view="base64" />
    </Key>

    <Sign>
      <objhash view="panhash" />
      <keyhash view="panhash" />
      <format view="byte" />
      <time view="date" />
      <sign view="base64" />
    </Sign>

    <Article>
      <text view="text" />
      <hash view="hex" />
      <author view="hex" />
    </Article>

  </Pandora>

</pandora-model>

<pandora-model>

  <World>

    <Person order="1">
      <first_name pos="1" fsize="20" />
      <last_name pos="+1" fsize="25" />
      <birth_day pos="|+1" />
      <birth_place pos="+0.10up" />
      <father_name pos="|up+0.3" />
      <mother_name pos="up+0.1" />
      <sex pos="|left" />
      <exit_day pos="left,hide" />
    </Person>

    <Community order="2">
      <name pos="1" fsize="84" />
      <origin_place pos="|+1" fsize="34" />
    </Community>

    <Article>
      <name pos="|" />
      <type pos="|" />
      <text view="text" />
      <hash view="hex" />
    </Article>

  </World>

  <Pandora name="Pandora Node">

    <Panobject>
      <panhash pos="|254,hide" view="phash" />
      <modified pos="255" view="time" />
    </Panobject>

    <Created>
      <creator pos="|252" view="phash" />
      <created pos="253" view="time" />
    </Created>

    <Parameter order="101" sort="section, name">
      <name pos="1" fsize="32" />
      <type pos="+1" />
      <section pos="+1" />
      <desc pos="|+1" fsize="64" />
      <value pos="|+1" fsize="64" />
      <setting pos="|+1" fsize="64" />
    </Parameter>

    <Key>
      <kind view="hex" />
      <expire view="time" />
      <cipher view="hex" />
      <body view="base64" />
    </Key>

    <Sign>
      <obj_hash view="panhash" />
      <key_hash view="panhash" />
      <packed view="hex" />
      <sign view="base64" />
    </Sign>

  </Pandora>

</pandora-model>
